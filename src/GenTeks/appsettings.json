{
  "ConnectionStrings": {
    "Content": "Data Source=.;Initial Catalog=Content;Integrated Security=True;TrustServerCertificate=true",
    "WorkFlow": "Data Source=.;Initial Catalog=Workflow;Integrated Security=True;TrustServerCertificate=true",
    "PublishingJob": "Data Source=.;Initial Catalog=PublishingJob;Integrated Security=True;TrustServerCertificate=true"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Debug"
      }
    },
    "WriteTo": [{ "Name": "Console" }],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationComponent": "GenTeks",
      "ApplicationIdentifier": "COVID"
    }
  }
}
