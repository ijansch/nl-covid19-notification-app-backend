{
  "Workflow": {
    "CleanupDeletesData": true,
    "PermittedMobileDeviceClockErrorMinutes": 30,
    "TimeToLiveMinutes": 1680
  },
  "ConnectionStrings": {
    "WorkFlow": "Data Source=.;Initial Catalog=Workflow;Integrated Security=True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Debug"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "ApplicationComponent": "GenTeks",
      "ApplicationIdentifier": "COVID"
    }
  }
}