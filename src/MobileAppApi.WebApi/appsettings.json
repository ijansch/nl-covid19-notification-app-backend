{
  "Workflow": {
    "PostKeys": {
      "TemporaryExposureKeys": {
        "Count": {
          "Max": 98,
          "Min": 1
        },
        "RollingStartNumber": {
          "Min": 2629584
        },
        "AuthorisationWindowMinutes": 120,
        "MaxAgeDays": 14,
        "PublishingDelayMinutes": 120
      }
    },
    "ResponsePadding": {
      "ByteCountMaximum": 300,
      "ByteCountMinimum": 200
    },
    "CleanupDeletesData": true,
    "PermittedMobileDeviceClockErrorMinutes": 30,
    "TimeToLiveMinutes": 1680
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "WorkFlow": "Data Source=.;Initial Catalog=Workflow;Integrated Security=True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "ApplicationComponent": "MobileAppApi",
      "ApplicationIdentifier": "COVID"
    }
  }
}
