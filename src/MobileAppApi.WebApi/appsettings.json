{
  "Workflow": {
    "PostKeys": {
      "TemporaryExposureKeys": {
        "Count": {
          "Max": 98,
          "Min": 1
        },
        "RollingStartNumber": {
          //"Min": 2629584 //disabled locally due to a bug when running PostWorkflowTest_InvalidSignature() in the pipeline
        },
        "AuthorisationWindowMinutes": 120,
        "MaxAgeDays": 14,
        "PublishingDelayMinutes": 120
      }
    },
    "CleanupDeletesData": true,
    "PermittedMobileDeviceClockErrorMinutes": 30,
    "TimeToLiveMinutes": 1680
  },
  "ConnectionStrings": {
    "WorkFlow": "Host=localhost;Database=Workflow;"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [{ "Name": "Console" }],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationComponent": "MobileAppApi",
      "ApplicationIdentifier": "COVID"
    }
  }
}
