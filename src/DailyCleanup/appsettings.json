{
  "ExposureKeySets": {
    "TekCount": {
      "Max": 750000,
      "Min": 150
    },
    "CleanupDeletesData": true,
    "LifetimeDays": 14,
    "PageSize": 10000
  },
  "Iks": {
    "LifetimeDays": 14
  },
  "Interop": {
    "Temp": {
      "AcceptableCountries": "BE,GR,LT,PT,BG,ES,LU,RO,CZ,FR,HU,SI,DK,HR,MT,SK,DE,IT,NL,FI,EE,CY,AT,SE,IE,LV,PL,IS,NO,LI,CH",
      "CountriesOfInterest": "BE,GR,LT,PT,BG,ES,LU,RO,CZ,FR,HU,SI,DK,HR,MT,SK,DE,IT,NL,FI,EE,CY,AT,SE,IE,LV,PL,IS,NO,LI,CH",
      "IksImportEnabled": "true"
    }
  },
  "Manifest": {
    "KeepAliveCount": 1
  },
  "Workflow": {
    "CleanupDeletesData": true,
    "PermittedMobileDeviceClockErrorMinutes": 30,
    "TimeToLiveMinutes": 1680
  },
  "ConnectionStrings": {
    "Content": "Data Source=.;Initial Catalog=Content;Integrated Security=True;TrustServerCertificate=true",
    "WorkFlow": "Data Source=.;Initial Catalog=Workflow;Integrated Security=True;TrustServerCertificate=true",
    "PublishingJob": "Data Source=.;Initial Catalog=PublishingJob;Integrated Security=True;TrustServerCertificate=true",
    "Stats": "Data Source=.;Initial Catalog=Stats;Integrated Security=True;TrustServerCertificate=true",
    "DiagnosisKeySource": "Data Source=.;Initial Catalog=DkSource;Integrated Security=True;TrustServerCertificate=true",
    "IksIn": "Data Source=.;Initial Catalog=IksIn;Integrated Security=True;TrustServerCertificate=true",
    "IksOut": "Data Source=.;Initial Catalog=IksOut;Integrated Security=True;TrustServerCertificate=true",
    "IksPublishingJob": "Data Source=.;Initial Catalog=IksPublishingJob;Integrated Security=True;TrustServerCertificate=true"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationComponent": "EksEngine",
      "ApplicationIdentifier": "COVID"
    }
  }
}
