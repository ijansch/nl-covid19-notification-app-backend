{
  "Content": {
    "ManifestCacheControl": "public, max-age=300, s-maxage=300",
    "ImmutableContentCacheControl": "public, immutable, max-age=1209600, s-maxage=1209600"
  },
  "ExposureKeySets": {
    "TekCount": {
      "Min": 150,
      "Max": 750000
    },
    "PageSize": 10000,
    "LifetimeDays": 14,
    "CleanupDeletesData": true
  },
  "TaskScheduling": {
    "DailyCleanupStartTime": "00:00:00"
  },
  "ConnectionStrings": {
    "Content": "Data Source=.;Initial Catalog=Content;Integrated Security=True"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "ApplicationComponent": "ContentApi",
      "ApplicationIdentifier": "COVID"
    }
  }
}
