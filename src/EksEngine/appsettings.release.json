{
  "ConnectionStrings": {
    "Content": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.Content.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "WorkFlow": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.WorkFlow.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "PublishingJob": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.PublishingJob.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "DiagnosisKeys": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.DkSource.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "IksIn": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.IksIn.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "IksOut": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.IksOut.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true",
    "IksPublishingJob": "Data Source=#{Common.DatabaseServer}#;Initial Catalog=#{Common.IksPublishingJob.DatabaseName}#;Integrated Security=True;TrustServerCertificate=true"
  },
  "Workflow": {
    "PostKeys": {
      "RollingStartNumber": {
        "Min": "#{Deploy.MobileAppApi.RollingStartNumber.Min}#"
      }
    }
  },
  "Interop": {
    "Temp": {
      "AcceptableCountries": "#{Deploy.Interop.AcceptableCountries}#",
      "CountriesOfInterest": "#{Deploy.Interop.CountriesOfInterest}#",
      "IksImportEnabled": "#{Deploy.Interop.IksImportEnabled}#"
    }
  },
  "Certificates": {
    "HsmSigner":
    {
      "BaseAddress": "",
      "CmsJwt": "",
      "CmsPublicCertificateChain": "",
      "GaenJwt": "",
      "GaenPublicCertificate": ""
    }
  },
  "ExposureKeySets": {
    "SignatureHeader": {
      "AppBundleId": "#{Deploy.EksEngine.AppBundleId}#",
      "VerificationKeyId": "#{Deploy.EksEngine.VerificationKeyId}#",
      "VerificationKeyVersion": "#{Deploy.EksEngine.VerificationKeyVersion}#",
      "VerificationKeyVersionV15": "#{Deploy.EksEngine.VerificationKeyVersionV15}#"
    },
    "TekCount": {
      "Min": "#{Deploy.EksEngine.TekCountMin}#",
      "Max": "#{Deploy.EksEngine.TekCountMax}#"
    },
    "LifetimeDays": "#{Deploy.EksEngine.LifetimeDays}#"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File",
      "Serilog.Sinks.Elasticsearch"
    ],
    "MinimumLevel": {
      "Default": "#{Deploy.EksEngine.Elk.MinimumLevel}#",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "#{Deploy.EksEngine.Elk.EFCore.Level}#",
        "Microsoft.EntityFrameworkCore.ChangeTracking": "#{Deploy.EksEngine.Elk.EFCore.Level}#"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "E:\\Log\\COVID\\EksEngine-%COMPUTERNAME%-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "fileSizeLimitBytes": 209715200,
          "rollOnFileSizeLimit": true
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://#{Deploy.Elk.Elasticsearch.Username}#:#{Deploy.Elk.Elasticsearch.Password}#@#{Deploy.Elk.ElasticsearchUrl1}#;http://#{Deploy.Elk.Elasticsearch.Username}#:#{Deploy.Elk.Elasticsearch.Password}#@#{Deploy.Elk.ElasticsearchUrl2}#",
          "indexFormat": "covid-{0:yyyy.MM.dd}",
          "restrictedToMinimumLevel": "#{Deploy.EksEngine.Elk.Elastic.RestrictedToMinimumLevel}#"
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationComponent": "EksEngine",
      "ApplicationIdentifier": "COVID"
    }
  }
}
