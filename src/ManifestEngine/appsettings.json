{
  "ConnectionStrings": {
    "Content": "Host=localhost;Database=content;"
  },
  "ExposureKeySets": {
    "TekCount": {
      "Max": 750000,
      "Min": 150
    },
    "CleanupDeletesData": true,
    "LifetimeDays": 14,
    "PageSize": 10000
  },
  "Certificates": {
    "HsmSigner":
    {
      "BaseAddress": "",
      "CmsJwt": "",
      "CmsPublicCertificateChain": ""
    }
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console"],
    "MinimumLevel": "Debug",
    "WriteTo": [{ "Name": "Console" }],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "ApplicationComponent": "ManifestEngine",
      "ApplicationIdentifier": "COVID"
    }
  }
}
